#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct {
    char name[100];
    char rating[10];
    char showTimes[5][10];
    int showTimesCount;
} Movie;

typedef struct {
    int age;
    float price;
    Movie selectedMovie;
    char selectedTime[10];
    int matinee;
} Ticket;
